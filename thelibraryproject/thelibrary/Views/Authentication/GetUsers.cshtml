@model IEnumerable<Users>

<table class="table">
    <tr>
        <th>Matric Number</th>
        <th>Name</th>
        @*<th>Author</th>*@
        <th>ID</th>
        @*<th>Action</th>*@
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td> @user.UserName</td>
            <td> @user.Name</td>
            <td>@user.Id</td>
                       

        </tr>
    }
</table>